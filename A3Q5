#include <stdio.h>
#include <ctype.h>
#define N 1000

int st[N],top=-1;

void push(int x){ st[++top]=x; }
int pop(){ return st[top--]; }

int main(){
    char s[1000];
    gets(s);
    for(int i=0;s[i];i++){
        if(isdigit(s[i])) push(s[i]-'0');
        else{
            int b=pop(),a=pop();
            if(s[i]=='+') push(a+b);
            else if(s[i]=='-') push(a-b);
            else if(s[i]=='*') push(a*b);
            else if(s[i]=='/') push(a/b);
        }
    }
    printf("%d",pop());
}
